{
  "webFetchAllowList": [
    "github.com",
    "githubusercontent.com",
    "gitlab.com",
    "bitbucket.org",
    "npmjs.com",
    "pypi.org",
    "crates.io",
    "go.dev",
    "pkg.go.dev",
    "stackoverflow.com",
    "stackexchange.com",
    "docs.rs",
    "readthedocs.io",
